{
  "name": "fenixedu-canvas",
  "description": "The most popular front-end framework for developing responsive, mobile first projects on the web.",
  "version": "1.0.0",
  "keywords": [
    "css",
    "js",
    "less",
    "mobile-first",
    "responsive",
    "front-end",
    "framework",
    "web",
    "fenixedu",
    "fenixedu-canvas"
  ],
  "repository": {
        "type": "git",
        "url": "git://github.com/fenixedu/fenixedu-canvas"
  },
  "homepage": "ttps://github.com/fenixedu/fenixedu-canvas",
  "main": [
    "less/bootstrap.less",
    "dist/css/bootstrap.css",
    "dist/js/bootstrap.js",
    "dist/fonts/glyphicons/glyphicons-halflings-regular.eot",
    "dist/fonts/glyphicons/glyphicons-halflings-regular.svg",
    "dist/fonts/glyphicons/glyphicons-halflings-regular.ttf",
    "dist/fonts/glyphicons/glyphicons-halflings-regular.woff",
    "dist/fonts/fenixedu-icons/*.{eot,svg,ttf,woff,txt}",
    "dist/fonts/tittilium/*.{eot,svg,ttf,woff,txt}",
    "dist/fonts/sourcesanspro/*.{eot,svg,ttf,woff,txt}",
    "dist/fancytree/**/*.*"
  ],
  "ignore": [
    ".*",
    "_config.yml",
    "CNAME",
    "composer.json",
    "CONTRIBUTING.md",
    "docs",
    "js/tests",
    "test-infra"
  ],
  "dependencies": {
    "jquery": ">= 1.9.0",
    "fancytree": "~2.2.0"
  }
}
